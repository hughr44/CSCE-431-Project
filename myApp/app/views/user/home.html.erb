<script src="https://cdn.jsdelivr.net/npm/animejs@3.0.1/lib/anime.min.js"></script>
<script>
    anime({
        targets: '.vertical-bar',
        delay: 1000,
        duration: 3000,
        translateX: ['0px', '100%'],
        // width: ['100%', '2px'],
        easing: 'linear'
    })
            
    for (let i = 0; i < 3; i++) {
        for (let j = 0; j < 13; j++) {
            let darkness = Math.ceil(255 * Math.random())
            
            if (i == 0) {
                if (Math.random() < 0.5) {
                    darkness = 255
                } else {
                    darkness = 0
                }
            }
            
            anime({
                targets: '.circle-' + (i+1) + '-' + (j+1),
                delay: (i+1) * 1000,
                duration: 500,
                backgroundColor: 'rgb('+darkness+','+darkness+','+darkness+')',
                easing: 'linear'
            })
        }
    }
    
    anime({
        targets: '.circle-4-1',
        delay: 4000,
        duration: 500,
        backgroundColor: 'rgb(25, 25, 25)',
        easing: 'linear'
    })
    
    anime({
        targets: '.circle-4-2',
        delay: 4000,
        duration: 500,
        backgroundColor: 'rgb(230, 230, 230)',
        easing: 'linear'
    })
</script>